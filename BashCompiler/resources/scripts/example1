PROCSUBVAR="--------->>>>>>>>"
PROCSUBVAR="${PROCSUBVAR}$(ls -1 /home) $(ls -1 /usr)"
#PROCSUBVAR="--------->>>>>>>>$(ls /home) $(ls /usr)<<<<<<<---------"
echo "$PROCSUBVAR"
PROCSUBVAR=$XVAR
echo $PROCSUBVAR