#>>>>>>>>>>>>>>>>>>
#echo "${XVAR}-${YVAR} 123456"

ls -1 /
ls -1 /|grep usr

LSVAR=$(ls ${HOME}/Desktop)

#------------------------
#echo "123 LSVAR=$LSVAR"
LSVAR="$(ls /) $(ls /home)"
