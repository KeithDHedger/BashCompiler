#!/bin/bash -e

#example 1

XVAR="123"
YVAR="abc def"
ZVAR=$YVAR
AVAR="${XVAR}${YVAR} 123456"
XVAR="666"
YVAR="xxx 111 333"
echo "$XVAR $XVAR"
